package StringClass;

public class launchFinal {
	public static void main(String args[]) {
		StringBuffer sc = new StringBuffer("Harshit");
		sc.append("Sahuwalya");
		System.out.println(sc);
		
		sc = new StringBuffer("Harsh");
		System.out.println(sc);
	}
}


















